<div class="container-fluid">
  <div class="row">
    <div class="col-md-4">
    <h4>Protocol domains</h4>
      <table class="table page-index-list">
      <thead>
        <tr>
          <th>Name</th>
          <th>Status</th>
          <th>Runtimes</th>
          <% for (i=0; i < runtimes.length - 1; i++) { %>
            <th></th>
          <% } %>
        </tr>
      </thead>
      <% domains.forEach(function(domain) { %>
        <tr>
          <td>
            <a href="/<%= domain.name %>"><%= domain.name %></a>
          </td>
          <td>
            <% if (domain.isExperimental) { %> 
              <span class="label label-warning">Experimental API</span>
            <% } %>
          </td>
          <% if (domain.runtimes) { %> 
            <% domain.runtimes.forEach(function(runtime) { %>
              <% if (runtime.protocol) { %> 
                <td class="runtime"><%= runtime.name %></td>
              <% } else { %>
                <td class="runtime"></td>
              <% } %>
            <% }) %>
          <% } %>
        </tr>
      <% }) %>
      </table>
    </div>
  </div>
</div>


