<div class="page-domain container-fluid">

  <div class="row">
    <div class="col-md-12">
      <h1><%= domain.name %> <% if (domain.isExperimental) { %> <span class="label label-warning">Experimental API</span> <% } %></h1>
      <p><%- domain.description %></p>
    </div>
  </div>

  <div class="info">
    <span class="label label-success">Common API</span>
    <span class="label label-warning">Experimental API</span>
    <span class="label label-danger">Not implemented</span>
  </div>

  <div class="row">    
    <div class="col-md-6">
      <h2></h2>
      <%- partial('table', { runtimes: domain.runtimes, items: commands, domain: domain, type: 'commands' }) %>
      
      <h2>Events</h2>
      <%- partial('table', { runtimes: domain.runtimes, items: events, domain: domain, type: 'events'}) %>
      
      <h2>Types</h2>
      <%- partial('table', { runtimes: domain.runtimes, items: types, domain: domain, type: 'types'}) %>
    </div>
  </div>
</div>


